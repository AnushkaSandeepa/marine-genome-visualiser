install.packages(c("jsonlite","remotes"))
m <- jsonlite::fromJSON("manifest.json")
# Build a data frame of Package + Version as recorded in your manifest
df <- do.call(rbind, lapply(m$packages, function(p) {
data.frame(Package = p$description$Package, Version = p$description$Version,
Repo = if (!is.null(p$Repository)) p$Repository else "https://cran.rstudio.com",
stringsAsFactors = FALSE)
}))
row.names(df) <- NULL
# Install each exact version from CRAN (or the recorded repo)
for (i in seq_len(nrow(df))) {
message("Installing ", df$Package[i], "==", df$Version[i])
remotes::install_version(df$Package[i], version = df$Version[i],
repos = df$Repo[i], upgrade = "never")
}
install.packages(c("jsonlite", "remotes"))
